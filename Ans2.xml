
using System;
					
public class Program
{
	public static void Main()
	{
		     string original = "abcdef";
			 string reverse = "";
       int length = original.Length;

        for ( int i = length - 1 ; i >= 0 ; i-- ){
	    reverse = reverse + original[i];
        } 
        Console.WriteLine("Reverse of entered string is: "+reverse);
	}
}




